[run]
source = agentformer
omit = 
    */tests/*
    */__init__.py
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except Exception as e:
        logger.error

[html]
directory = coverage_html 